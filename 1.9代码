1.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
void resver(int n)
{
	if (n > 9)
	{
		resver(n / 10);
	}
	printf("%d ", n % 10);
}
int main()
{
	//给出一个不多于5位的整数，要求 1、求出它是几位数 2、分别输出每一位数字 3、按逆
	//序.输出各位数字，例如原数为321, 应输出123
		
	int num = 0;
	int count = 0;
	scanf("%d", &num);
	int temp = num;
	int temp1 = num;
	while (num > 0)
	{
		num = num / 10;
		count++;
	}
	printf("%d\n", count);
	resver(temp1);
	printf("\n");
	while (temp > 0)
	{
		printf("%d", temp % 10);
		temp /= 10;
	}
	printf("\n");
      system("pause");
    return 0;
}
2.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int main()
{
	//给出一百分制成绩，要求输出成绩等级‘A’、‘B’、‘C’、‘D’、‘E’。 90分以及90分以上为A，80 - 89分为B，70 - 79分为C，60 - 69分为D，60分以下为E。
		int n = 0;
	scanf("%d", &n);
	if (n >= 90)
	{
		printf("A");
	}
	else if (n < 90 && n >= 80)
	{
		printf("B");
	}
	else if (n < 80 && n >= 70)
	{
		printf("C");
	}
	else if (n < 70 && n >= 60)
	{
		printf("D");
	}
	else
	{
		printf("E");
	}
      return 0;
}
3.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int main()
{
	int x, y;
	scanf("%d", &x);
	if (x < 1)
	{
		y = x;
	}
	else if (x >= 1 && x <= 10)
	{
		y = 2 * x - 1;
	}
	else if (x >= 10)
	{
		y = 3 * x - 11;
	}
	printf("%d", y);
      system("pause");
    return 0;
}
4.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
	int max1(int a, int b)
	{
		return a > b ? a : b;
	}
int main()
{
	int a, b, c;
	scanf("%d%d%d", &a, &b, &c);
	int temp = max1(max1(a, b), c);
	printf("%d", temp);
    system("pause");
    return 0;
}
5.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int main()
{
	float F = 0;
	scanf("%f", &F);
	float c;
	c = 5 * (F - 32) / 9;
	printf("c=%.2f", c);
      system("pause");
    return 0;
}
6.
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int main()
{
	char c1, c2, c3, c4, c5;
	scanf("%c%c%c%c%c", &c1, &c2, &c3, &c4, &c5);
	c1 = c1 + 4;
	c2 = c2 + 4;
	c3 = c3 + 4;
	c4 = c4 + 4;
	c5 = c5 + 4;
	printf("%c%c%c%c%c", c1, c2, c3, c4, c5);
      system("pause");
    return 0;
}
